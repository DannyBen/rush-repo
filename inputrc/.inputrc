# Change up/down arrows behavior
"\e[A": history-search-backward
"\e[B": history-search-forward

# Change tab behavior and add shift tab for old behavior
TAB: menu-complete
"\e[Z": complete

# Ctrl right/left to move between words
# "\e[C": forward-word
# "\e[D": backward-word

# Alt-s to add run command with sudo
# "\es": "\C-asudo \C-M"

