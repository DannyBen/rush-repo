#!/usr/bin/env bash
set -e

# Personal packages
rush $REPO my/bashrc
rush $REPO my/cdpath
rush $REPO my/gitconfig
rush $REPO my/rigit
rush $REPO my/rush
# rush $REPO my/ssh-agent

# Public packages
rush $REPO dircolors
rush $REPO exrc
rush $REPO inputrc
rush $REPO irbrc

# Install only if necessary
[[ -x "$(command -v alf)" ]]    || rush $REPO my/alf

printf "\nDone. You may need to login again.\n"
