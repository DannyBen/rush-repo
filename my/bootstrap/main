#!/usr/bin/env bash
source ../../common.sh

say "installing my/* packages"
needs my/bashrc my/cdpath my/gitconfig my/rigit my/bash-completion

say "installing public packages"
needs ssh-agent dircolors exrc inputrc toprc locale

command_exist alf || rush get "$REPO:my/alf"

say "my/bootstrap complete, you may need to login again"
